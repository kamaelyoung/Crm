@using Crm.Website;
@using Crm.Website.Models;
@{
    ViewBag.Title = "联系人信息导入";
    Layout = "~/Views/Contact/Layout.cshtml";
    this.ViewBag.LeftMenu = "Import";
}
<p class="lead">联系人信息导入</p>
<form enctype="multipart/form-data" action="@this.Url.Action("UploadImportFile")" method="post">
     <fieldset>
        <legend>第一步</legend>
        <a class="btn" href="@this.Url.Action("DownloadImportTemplate")"><i class="icon-download"></i>下载模板</a>
    </fieldset>
     <fieldset>
        <legend>第二步</legend>
        <input type="file" name="importFile"/>
        <button class="btn"><i class="icon-upload"></i>上传</button>
    </fieldset>
</form>