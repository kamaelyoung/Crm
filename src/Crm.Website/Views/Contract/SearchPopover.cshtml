<div class="popover bottom" style="width:400px;max-width:400px;">
    <div class="arrow">
    </div>
    <h3 class="popover-title">查询合同</h3>
    <div class="popover-content" style="width:400px;">
        <form class="form-horizontal">
            <div class="control-group">
                <label style="width: 80px" class="control-label">开始时间</label>
                <div style="margin-left: 120px;" class="controls">
                    <input type="text" name="startDateRangeStart" class='date input-small'/>到
                    <input type="text" name="startDateRangeEnd" class='date input-small'/>
                </div>
            </div>
            <div class="control-group">
                <label style="width: 80px" class="control-label">结束时间</label>
                <div style="margin-left: 120px;" class="controls">
                    <input type="text" name="endDateRangeStart" class='date input-small'/>到
                    <input type="text" name="endDateRangeEnd" class='date input-small'/>
                </div>
            </div>
            <div class="control-group">
                <label style="width: 80px" class="control-label">合同金额</label>
                <div style="margin-left: 120px;" class="controls">
                    <input type="text" name="valueRangeMin" class='input-small'/>到
                    <input type="text" name="valueRangeMax" class='input-small'/>
                </div>
            </div>
            <div class="control-group">
                <label style="width: 80px" class="control-label">关键字</label>
                <div style="margin-left: 120px;" class="controls">
                    <input type="text" name="keyword" />
                </div>
            </div>
            <div class="control-group">
                <div style="margin-left: 120px;" class="controls">
                    <button type="submit" class="btn btn-primary">确定</button>
                    <button type="button btnCancel" class="btn">取消</button>
                    <button type="reset" class="btn">重置</button>
                </div>
            </div>
        </form>
    </div>
</div>