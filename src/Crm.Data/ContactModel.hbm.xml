<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="Crm.Data" assembly="Crm.Data">
    <class name="ContactModel" table="crm_contact">
        <id name="ID" type="string" column="contact_id">
            <generator class="uuid.hex">
            </generator>
        </id>
        <property name="Name" type="String" column="contact_name"></property>
        <property name="CustomerId" type="string" column="cust_id"></property>
        <property name="CreatorId" type="String" column="contact_creatorId"></property>
        <property name="CreateTime" type="DateTime" column="contact_createTime"></property>
        <property name="ModifiedUserId" type="String" column="contact_modifiedUserId"></property>
        <property name="ModifiedTime" type="DateTime" column="contact_modifiedTime"></property>
        <property name="MetadataId" type="int" column="metadata_id"></property>
    </class>
</hibernate-mapping>